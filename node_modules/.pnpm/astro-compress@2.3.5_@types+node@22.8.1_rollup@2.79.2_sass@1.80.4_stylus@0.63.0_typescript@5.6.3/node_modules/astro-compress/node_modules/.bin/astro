#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yuyu/brother_blog/node_modules/.pnpm/astro@4.16.10_@types+node@22.8.1_lightningcss@1.27.0_rollup@2.79.2_sass@1.80.4_stylus@0.63.0__a2wbs2hyq3rfchtrx622wr7fxm/node_modules/astro/node_modules:/home/yuyu/brother_blog/node_modules/.pnpm/astro@4.16.10_@types+node@22.8.1_lightningcss@1.27.0_rollup@2.79.2_sass@1.80.4_stylus@0.63.0__a2wbs2hyq3rfchtrx622wr7fxm/node_modules:/home/yuyu/brother_blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yuyu/brother_blog/node_modules/.pnpm/astro@4.16.10_@types+node@22.8.1_lightningcss@1.27.0_rollup@2.79.2_sass@1.80.4_stylus@0.63.0__a2wbs2hyq3rfchtrx622wr7fxm/node_modules/astro/node_modules:/home/yuyu/brother_blog/node_modules/.pnpm/astro@4.16.10_@types+node@22.8.1_lightningcss@1.27.0_rollup@2.79.2_sass@1.80.4_stylus@0.63.0__a2wbs2hyq3rfchtrx622wr7fxm/node_modules:/home/yuyu/brother_blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astro@4.16.10_@types+node@22.8.1_lightningcss@1.27.0_rollup@2.79.2_sass@1.80.4_stylus@0.63.0__a2wbs2hyq3rfchtrx622wr7fxm/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../astro@4.16.10_@types+node@22.8.1_lightningcss@1.27.0_rollup@2.79.2_sass@1.80.4_stylus@0.63.0__a2wbs2hyq3rfchtrx622wr7fxm/node_modules/astro/astro.js" "$@"
fi
